std::string Style::CnfPrefixToName(const std::string &amp;prefix) {
    std::string name = "#def-";

    for(size_t i = 0; i &lt; prefix.length(); i++) {
        if(isupper(prefix[i]) &amp;&amp; i != 0)
            name += '-';
        name += tolower(prefix[i]);
