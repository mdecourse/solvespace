<t tx="leo.20191228122645.42">void Group::AddEq(IdList&lt;Equation,hEquation&gt; *l, Expr *expr, int index) {
    Equation eq;
    eq.e = expr;
    eq.h = h.equation(index);
    l-&gt;Add(&amp;eq);
}

