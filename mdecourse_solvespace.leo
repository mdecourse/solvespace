    Vector n = splane-&gt;NormalAt(0, 0).WithMagnitude(1), along;
    double d = n.Dot(splane-&gt;PointAt(0, 0));
    SBezier bezier;
    (void)sext-&gt;IsExtrusion(&amp;bezier, &amp;along);

    if(fabs(n.Dot(along)) &lt; LENGTH_EPS) {
