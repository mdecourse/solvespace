    if(srf-&gt;LineEntirelyOutsideBbox(ea, eb, /*asSegment=*/true)) continue;

    Point2d puv;
    srf-&gt;ClosestPointTo(p, &amp;(puv.x), &amp;(puv.y), /*mustConverge=*/false);
    Vector pp = srf-&gt;PointAt(puv);

