    Entity *e = SK.entity.FindByIdNoOops(gs.entity[0]);
    if(!e || e-&gt;type != Entity::Type::TTF_TEXT || !e-&gt;h.isFromRequest()) return;

    Request *r = SK.request.FindByIdNoOops(e-&gt;h.request());
