<t tx="leo.20191228122647.107">void System::EvalJacobian() {
    int i, j;
    for(i = 0; i &lt; mat.m; i++) {
        for(j = 0; j &lt; mat.n; j++) {
            mat.A.num[i][j] = (mat.A.sym[i][j])-&gt;Eval();
        }
    }
}

