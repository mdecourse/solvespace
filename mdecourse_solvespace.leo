<t tx="leo.20191228122643.18">void GraphicsWindow::MakeUnselected(hEntity he, bool coincidentPointTrick) {
    Selection stog = {};
    stog.entity = he;
    MakeUnselected(&amp;stog, coincidentPointTrick);
