double SMesh::CalculateSurfaceArea(const std::vector&lt;uint32_t&gt; &amp;faces) const {
    double area = 0.0;
    for(uint32_t f : faces) {
        for(const STriangle &amp;t : l) {
            if(f != t.meta.face) continue;
            area += t.Area();
        }
    }
    return area;
}
