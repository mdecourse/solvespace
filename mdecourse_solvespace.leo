    if(fabs(dotp[1]) &lt; DOTP_TOL) {
        swap(dotp[0],         dotp[1]);
        swap(inter_surf_n[0], inter_surf_n[1]);
        swap(inter_edge_n[0], inter_edge_n[1]);
