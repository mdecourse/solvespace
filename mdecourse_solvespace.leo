    STriangleLl *tll = NULL;
    for(i = 0; i &lt; m-&gt;l.n; i++) {
        STriangleLl *tn = STriangleLl::Alloc();
        tn-&gt;tri = &amp;(tra[i]);
        tn-&gt;next = tll;
        tll = tn;
    }

    return SKdNode::From(tll);
}

