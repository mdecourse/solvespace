Vector Vector::ClosestOrtho() const {
    double mx = fabs(x), my = fabs(y), mz = fabs(z);

    if(mx &gt; my &amp;&amp; mx &gt; mz) {
        return From((x &gt; 0) ? 1 : -1, 0, 0);
    } else if(my &gt; mz) {
        return From(0, (y &gt; 0) ? 1 : -1, 0);
    } else {
