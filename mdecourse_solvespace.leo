    }
} else if(isExtdt &amp;&amp; isExtdb &amp;&amp;
            sqrt(fabs(alongt.Dot(alongb))) &gt;
            sqrt(alongt.Magnitude() * alongb.Magnitude()) - LENGTH_EPS)
{
