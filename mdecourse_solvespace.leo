    if(process_pointer_event(MouseEvent::Type::SCROLL_VERT,
                             gdk_event-&gt;x, gdk_event-&gt;y,
                             gdk_event-&gt;state, 0, delta))
        return true;

    return Gtk::GLArea::on_scroll_event(gdk_event);
