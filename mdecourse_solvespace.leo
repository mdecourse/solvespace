<t tx="leo.20191228122645.52">void SShell::RemapFaces(Group *g, int remap) {
    SSurface *ss;
    for(ss = surface.First(); ss; ss = surface.NextAfter(ss)){
        hEntity face = { ss-&gt;face };
        if(face == Entity::NO_ENTITY) continue;

        face = g-&gt;Remap(face, remap);
        ss-&gt;face = face.v;
    }
}

