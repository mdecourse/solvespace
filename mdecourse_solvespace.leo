<t tx="leo.20191228122650.171">static int RoundUpToPowerOfTwo(int v)
{
    for(int i = 0; i &lt; 31; i++) {
        int vt = (1 &lt;&lt; i);
        if(vt &gt;= v) {
            return vt;
        }
    }
    return 0;
}

