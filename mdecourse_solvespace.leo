        SBezier bef, aft;
        SplitAt(0.5, &amp;bef, &amp;aft);
        bef.MakeNonrationalCubicInto(bl, tolerance, depth+1);
        aft.MakeNonrationalCubicInto(bl, tolerance, depth+1);
    }
