    double rads = st-&gt;textAngle*PI/180;
    double c = cos(rads), s = sin(rads);
    Vector pr = gr, pu = gu;
    gr = pr.ScaledBy( c).Plus(pu.ScaledBy(s));
    gu = pr.ScaledBy(-s).Plus(pu.ScaledBy(c));
