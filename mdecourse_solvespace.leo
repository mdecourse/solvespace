<t tx="leo.20191228122650.169">void OpenGl1Renderer::SelectPrimitive(GLenum mode) {
    if(current.drawing &amp;&amp; current.mode == mode) {
        return;
    } else if(current.drawing) {
        glEnd();
    }
    glBegin(mode);
    current.drawing = true;
    current.mode    = mode;
}

