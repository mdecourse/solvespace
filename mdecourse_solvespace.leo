<t tx="leo.20191228122646.16">void GraphicsWindow::StartDraggingBySelection() {
    List&lt;Selection&gt; *ls = &amp;(selection);
    for(Selection *s = ls-&gt;First(); s; s = ls-&gt;NextAfter(s)) {
        if(!s-&gt;entity.v) continue;

