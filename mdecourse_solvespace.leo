<t tx="leo.20191228122651.44">void SSurface::MakeClassifyingBsp(SShell *shell, SShell *useCurvesFrom) {
    SEdgeList el = {};

    MakeEdgesInto(shell, &amp;el, MakeAs::UV, useCurvesFrom);
    bsp = SBspUv::From(&amp;el, this);
